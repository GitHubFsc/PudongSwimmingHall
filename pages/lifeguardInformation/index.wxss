/* pages/lifeguard/index.wxss */
.container {
  width: 100%;
  height: 100%;
  overflow: scroll;
  background-color: #FBFBFB;
  position: relative;
}
.nav {
  height: 0;
}
.bgimg {
  width: 100%;
  height: auto;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.main{
  width: auto;
  height: auto;
  position: absolute;
  top: 170rpx;
  left: 25rpx;
  z-index: 2;
}
.body{
  width: 700rpx;
  height: auto;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.body .title{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size:36rpx;
  color:rgba(0,0,0,1);
  border-bottom: 1rpx solid #f0f0f0;
}
.body .empty_box{
  text-align: center;
  font-size:26rpx;
  font-weight:400;
  color:rgba(223,223,223,1);
  padding-bottom: 90rpx;
}
.body .empty{
  width: 120rpx;
  height: 120rpx;
  margin: 30rpx auto 10rpx;
}
.main .prompt{
  text-align: center;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(186,186,186,1);
}
.main .btns {
  width: auto;
  height: auto;
  padding: 100rpx 0;
}
.main .btns .btn{
  width:340rpx;
  height:80rpx;
  margin: 0 auto;
  background:rgba(78,164,251,1);
  border-radius:40rpx;
  font-size:38rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 80rpx;
}